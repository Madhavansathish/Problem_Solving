#include <stdio.h>
#include <string.h>
int main()
{
    int t;
    scanf("%d",&t);
    while(t--){
        char str[105];
        scanf("%s",str);
       
        int n=strlen(str);
        int half=n/2;
        
        if(n%2!=0){
            printf("NO\n");
            continue;
        }
        
        int flag=1;
        for(int i=0;i<half;i++){
            if(str[i]!=str[i+half]){
                flag=0;
                break;
            }
        }
        
        if(flag){
            printf("YES\n");
        }
        else{
            printf("NO\n");
        }
    }
}
