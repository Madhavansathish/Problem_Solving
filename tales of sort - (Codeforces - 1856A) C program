#include <stdio.h>

int main()
{
    int t;
    scanf("%d",&t);
    
    while(t--){
        int n;
        scanf("%d\n",&n);
        int arr[n];
        for(int i=0;i<n;i++){
            scanf("%d",&arr[i]);
        }
        int max=0;
        for(int i=0;i<n-1;i++){
            if(arr[i]>arr[i+1]){
                if(arr[i]>max){
                    max=arr[i];
                }
            }
        }
        printf("%d\n",max);
    }
}
