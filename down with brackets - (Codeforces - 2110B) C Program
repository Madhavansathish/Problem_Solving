#include <stdio.h>
#include <string.h>
 
int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        char s[200005];
        scanf("%s", s);
        int n = strlen(s);
 
        if (s[0] == ')' || s[n - 1] == '(') {
            printf("YES\n");
            continue;
        }
 
        int balance = 0;
        int primitive = 1;  
        for (int i = 0; i < n - 1; i++) { 
            balance += (s[i] == '(' ? 1 : -1);
            if (balance == 0) { 
                primitive = 0;
                break;
            }
        }
 
        if (primitive)
            printf("NO\n");
        else
            printf("YES\n");
    }
    return 0;
}
