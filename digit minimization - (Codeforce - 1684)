#include <stdio.h>

int main()
{
    int n;
    scanf("%d",&n);
    while(n--){
        int num;
        scanf("%d",&num);
        int temp=num;
        int temp2=num;
        int count=0;
        while(num>0){
            count++;
            num/=10;
        }
        int min=9;
        if(count>2){
            while(temp>0){
                int d=temp%10;
                if(d<min)
                   min=d;
                temp/=10;
            }
            printf("%d\n",min);
        }else{
            min=temp%10;
            printf("%d\n",min);
        }
        }
    
}
