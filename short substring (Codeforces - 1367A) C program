#include <stdio.h>
#include <string.h>
int main()
{
    int t;
    scanf("%d",&t);
    while(t--){
        char a[105];
        scanf("%s",a);
        
        int n=strlen(a);
        int idx=0;
        
        char b[105];
        b[idx++]=a[0];
        
        for(int i=1;i<n;i+=2){
            b[idx]=a[i];
            idx++;
        }
        b[idx]='\0';
        printf("%s\n",b);
    }
}
