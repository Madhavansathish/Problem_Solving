#include <stdio.h>
#include <string.h>

int main() {
    int n;
    scanf("%d", &n);
    getchar(); // consume leftover newline

    char str[100][100];

    // input n strings
    for (int i = 0; i < n; i++) {
        fgets(str[i], sizeof(str[i]), stdin);
        str[i][strcspn(str[i], "\n")] = '\0';  // remove newline
    }

    // process each string
    for (int k = 0; k < n; k++) {
        // print first letter of the line
        printf("%c", str[k][0]);

        // check rest of the string
        int i = 0;
        while (str[k][i] != '\0') {
            if (str[k][i] == ' ') {
                printf("%c", str[k][i+1]);  // print next letter after space
            }
            i++;
        }
        printf("\n"); // go to new line for next acronym
    }

    return 0;
}
